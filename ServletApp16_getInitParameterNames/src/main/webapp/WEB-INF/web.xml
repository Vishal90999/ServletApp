	    </init-param>
	 </filter>

<filter-mapping>
  <filter-name>DisplayFilter</filter-name>
  <url-pattern>/login</url-pattern>
</filter-mapping>

<welcome-file-list>
   <welcome-file>input.html</welcome-file>
</welcome-file-list>



</web-app>